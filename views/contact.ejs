    <div class="footer-main">
      <div class="footer-icon">
        <div class="col-md-4 footer-grid">
          <span class="glyphicon glyphicon-phone-alt call-icon"></span>
          <div class="footer-text">
            <p>
              电话：
              <span class="foot"><%= doc.tel %></span>
            </p>
          </div>
          <div class="clearfix"></div>
        </div>
        <div class="col-md-4 footer-grid">
          <span class="glyphicon glyphicon-envelope call-icon"></span>
          <div class="footer-text">
            <p>
              邮箱：
              <span class="foot"><%= doc.email %></span>
            </p>
          </div>
          <div class="clearfix"></div>
        </div>
        <div class="col-md-4 footer-grid">
          <span class="glyphicon glyphicon-home call-icon" aria-hidden="true"></span>
          <div class="footer-text">
            <p>
              地址：
              <span class="foot"><%= doc.address %></span>
