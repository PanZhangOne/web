</head>
<body>
<!--顶部导航开始-->
<nav class="navbar navbar-default navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand active" href="/admin">Home</a>
    </div>
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
        <li class=""><a href="/">首页 <span class="sr-only">(current)</span></a></li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">用户 <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li role="separator" class="divider"></li>
            <li><a href="/signout">退出登陆</a></li>
          </ul>
        </li>
      </ul>
    </div>
  </div>
</nav>
<!--顶部导航结束-->
<!--侧边栏开始-->
<div class="wrap">
  <div class="admin-sidebar sidebar">
    <div class="admin-offcanvar-bar">
      <ul class="am-list">
        <li>
          <a href="/admin">首页</a>
        </li>
        <li class="js-list">
          <a href="javascript:void(0)">文章管理</a>
          <ul class="admin-sidebar-sub" style="display:none;">
            <li>
              <a href="/posts">新建文章</a>
            </li>
            <li>
              <a href="/adposts">管理中心</a>
            </li>
          </ul>
        </li>
        <li class="js-list">
          <a href="javascript:void (0)">项目展示</a>
          <ul class="admin-sidebar-sub" style="display: none;">
            <li>
              <a href="#">新增项目</a>
            </li>
            <li>
              <a href="#">管理中心</a>
            </li>
          </ul>
        </li>
        <li class="js-list">
          <a href="javascript:void(0)">咨询管理</a>
          <ul class="admin-sidebar-sub" style="display: none">
            <li>
              <a href="/leave/off">未操作</a>
            </li>
            <li>
              <a href="/leave/ok">已操作</a>
            </li>
          </ul>
        </li>
        <li class="js-list">
          <a href="javascript:void(0)">团队相册</a>
          <ul class="admin-sidebar-sub" style="display: none;">
            <li>
              <a href="#">管理</a>
            </li>
          </ul>
        </li>
        <li class="js-list">
          <a href="#">设计管理</a>
          <ul class="admin-sidebar-sub" style="display: none">
            <li>
              <a href="/designer">添加设计师</a>
            </li>
            <li><a href="#">设计师列表</a></li>
          </ul>
        </li>
      </ul>
    </div>
  </div>
  <!--侧边栏结束-->
  <!--后台主体-->
  <div class="admin-content">
    <div class="ad-content-body">
      <div class="body-header">
        <div class="bh-nav">
          <strong>留言</strong>/ <small>已经操作的留言</small>
        </div>
      </div>
      <div class="body-main">
        <table class="table table-bordered">
          <tr>
            <th>姓名</th>
            <th>联系方式</th>
            <th>时间</th>
            <th>留言</th>
            <th>备注</th>
            <th>操作</th>
          </tr>
          <% for (var i = 0;i < doc.length;i++){ %>
          <tr>
            <td><%= doc[i].name%></td>
            <td><%= doc[i].tel%></td>
            <td><%= doc[i].time %></td>
            <td><%= doc[i].message %></td>
            <td><%= doc[i].remarks %></td>
            <td>回访</td>
          </tr>
          <% } %>
        </table>
      </div>
    </div>
  </div>
</div>
<script src="/js/jquery-2.2.1.js"></script>
<script src="/js/admin.js"></script>
<% include footer %>
